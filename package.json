{
    "name": "@hapi/hoek",
    "description": "General purpose node utilities",
    "version": "8.4.0",
    "repository": "git://github.com/hapijs/hoek",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "keywords": [
        "utilities"
    ],
    "files": [
        "lib"
    ],
    "dependencies": {},
    "devDependencies": {
        "@hapi/code": "6.x.x",
        "@hapi/lab": "20.x.x",
        "typedoc": "^0.15.0",
        "typedoc-plugin-markdown": "^2.2.11"
    },
    "scripts": {
        "test": "lab -a @hapi/code -t 100 -L -Y",
        "test-cov-html": "lab -a @hapi/code -t 100 -L -r html -o coverage.html",
        "generateDocs": "typedoc docs lib --readme none --target ES6 --plugin typedoc-plugin-markdown --includeDeclarations --disableOutputCheck --excludeExternals -hideSources -hideBreadcrumbs"
    },
    "license": "BSD-3-Clause"
}
